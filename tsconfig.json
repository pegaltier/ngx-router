{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "module": "esnext",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "importHelpers": true,
        "strict": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "target": "es2015",
        "typeRoots": ["node_modules/@types"],
        "lib": ["es2018", "dom"],
        "paths": {
            "ngx-router/route-param": ["projects/ngx-router/route-param/index"],
            "ngx-router/route-param-snapshot": [
                "projects/ngx-router/route-param-snapshot/index"
            ],
            "ngx-router/query-param": ["projects/ngx-router/query-param/index"],
            "ngx-router/query-param-snapshot": [
                "projects/ngx-router/query-param-snapshot/index"
            ]
        }
    }
}
